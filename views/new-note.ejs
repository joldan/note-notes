<%- include('./partials/header') %> 
    <title>Awesome Notepad</title>
</head>
<body>
    <h1><%= title %> </h1>
    <form action="/" method="GET">
        <button type="submit" class="myButton"> Cancel </button>
    </form>
    <div class="card">
        <form action="new-note" method="POST" class="form-control">
            <label>Title</label>
            <input type="text" name="title" autocomplete="off">
            <label>Content</label>
            <textarea name="content" rows="5"> </textarea>
                <button type="submit" class="myButton"> Save </button>
        </form>
    </div>
</body>
<%- include('./partials/footer') %> 
